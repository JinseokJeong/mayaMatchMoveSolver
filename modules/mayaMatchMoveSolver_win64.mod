+ PLATFORM:@MODULE_OS_NAME@ MAYAVERSION:@MAYA_VERSION@ @PROJECT_NAME@ @PROJECT_VERSION@ ./@MODULE_FULL_NAME@
MMSOLVER_LOCATION :=
MMSOLVER_CONFIG_PATH += ${APPDATA}/mmSolver
MMSOLVER_CONFIG_PATH +:= config
MMSOLVER_RESOURCE_PATH := resources
MMSOLVER_LOAD_AT_STARTUP = 1
MMSOLVER_CREATE_MENU = 1
MMSOLVER_CREATE_SHELF = 1
MMSOLVER_CREATE_HOTKEY_SET = 1
MMSOLVER_VIEWPORT_MESSAGES = 1
MMSOLVER_HELP_SOURCE = local
MMSOLVER_DEFAULT_SOLVER = @DEFAULT_SOLVER@
MMSOLVER_DEBUG = 0
MAYA_CUSTOM_TEMPLATE_PATH +:= scripts/AETemplates
PATH +:= lib
PYTHONPATH +:= python
[r] scripts: scripts
